{
  "name": "movieapp",
  "version": "1.0.0",
  "main": "src/index.tsx",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "scripts": {
    "test": "jest",
    "testCoverage": "jest --coverage",
    "start:prod": "cross-env NODE_ENV=production webpack-dev-server --config webpack.config.js --mode=production",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "serve": "webpack serve --mode=development",
    "lint:test": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --fix --quiet --ext .ts,.tsx",
    "prepare": "cd.. && husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FallenBY1/reactJS2022Q4CIS.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FallenBY1/reactJS2022Q4CIS/issues"
  },
  "homepage": "https://github.com/FallenBY1/reactJS2022Q4CIS#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,less}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,scss,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "description": "My webpack project",
  "dependencies": {
    "@billjs/query-string": "^1.0.2",
    "@redux-devtools/extension": "^3.2.3",
    "@types/query-string": "^6.3.0",
    "@types/react-modal": "^3.13.1",
    "formik": "^2.2.9",
    "i18next": "^22.0.4",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "redux": "^4.2.0",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.4.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@webpack-cli/generators": "^2.5.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.3.1",
    "babel-loader": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-webpack-plugin": "^0.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-boundaries": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-loader": "^4.2.0",
